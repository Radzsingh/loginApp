<h1>Sign In</h1>
<%= form_with url: sign_in_path do |f| %>
  <p>
  <%= f.label 'Username:'%><br>
  <%= f.text_field :user_name, id: 'user_name', required: true %>
  </p>
  <p>
  <%= f.label 'password:'%><br>
  <%= f.password_field :password, id: 'password', required: true %>
  </p>
  <p>
  <%= f.submit 'Log In' %>
  </p>
<% end %>
